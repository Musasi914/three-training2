{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/src/examples/:path*",
      "destination": "/src/examples/:path*"
    }
  ],
  "headers": [
    {
      "source": "/src/examples/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
